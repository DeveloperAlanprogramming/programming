void eliminar_posiciones(){


	vector<int> numeros;
	/*metodos para ingresar*/

	for(int i=0; i<20; i++){

		numeros.push_back(i);
	}

	/*impresion de los datos*/
	for(int i=0; i< numeros.size();i++){

		cout<<numeros[i]<<"|";
	}

	cout<<endl;

	numeros.erase(numeros.begin() + 1, numeros.begin() + 3);
	for (int i = 0; i < numeros.size(); i++) {

		cout << numeros[i] << "|";
	}

	cout<<"\n";
}

